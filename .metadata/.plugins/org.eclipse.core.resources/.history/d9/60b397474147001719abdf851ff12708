package Lines;


import java.awt.event.MouseListener;

import javax.swing.JFrame;

public class Paint {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Dda d = new Dda();
		JFrame f = new JFrame();
		
		GridMouse mouse = new GridMouse();
		f.add(d);
		f.setSize(800, 800);
		f.setVisible(true);
		f.addMouseMotionListener(mouse);
		;
	}

}
