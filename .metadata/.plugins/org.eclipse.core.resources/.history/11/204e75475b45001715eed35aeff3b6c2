import java.awt.Color;
import java.awt.Graphics;

public class Dda {
	public Dda() {

	}

	public static void draw(int x1, int y1, int x2, int y2, int count, Graphics g) {
		int m = (x1 - x2) / (y1 - y2);

		for (int i = 0; i < count; i++) {
			Math.ceil(x1);
			Math.ceil(y1);
			g.setColor(Color.WHITE);
			g.fillRect(x1 * i * m, y1 * i * m, 10, 10);
		}
	}
}
